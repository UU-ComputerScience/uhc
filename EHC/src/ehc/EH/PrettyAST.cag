                loc     .  info     =   []
                        .  info_2   =   []
                        .  info_3   =   []
                        .  info_4_2 =   []
                        .  info_8   =   []
                        .  info_9   =   []
                        .  info_99  =   []

SEM Decl
  | Val         lhs     .  ppAST    =   ppNestInfo @lhs.opts ["Decl","Val"] [] [@patExpr.ppAST,@expr.ppAST] (@info_1 ++ @info_2 ++ @info_8 ++ @info_9)
                loc     .  info_1   =   []
                        .  info_2   =   []
                        .  info_8   =   []
                        .  info_9   =   []
  | TySig       lhs     .  ppAST    =   ppNestInfo @lhs.opts ["Decl","TySig"] [ppNm @nm] [@tyExpr.ppAST] (@info_1 ++ @info_5 ++ @info_6)
                loc     .  info_1   =   []
                        .  info_5   =   []
