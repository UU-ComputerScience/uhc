-------------------------------------------------------------------------
-- Common to other optimisations: is in composite dest?
-------------------------------------------------------------------------

ATTR AEqnDest AEqnDests [ isInComposite: Bool | | ]

SEM AEqn
  | Eqn             dest    .   isInComposite
                                            =   False

SEM AEqnDest
  | Many            dests   .   isInComposite
                                            =   True

